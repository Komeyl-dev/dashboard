{
    "name": "komeyl/dashboard",
    "description": "This is a basic laravel dashboard for speed project",
    "keywords": ["laravel", "admin", "panel" , "dashboard"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "komeyl kalbali",
            "email": "komeyl.kalbali.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "davejamesmiller/laravel-breadcrumbs": "5.x"
    },
    "autoload": {
        "files": [
            "src/Http/Helpers/helperFun.php"
        ],
        "classmap": [
            "src/Database/"
        ],
        "psr-4": {
            "App\\": "app/",
            "Komeyl\\Dashboard\\": "src/"
        }
    },
	"repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org/packages/komeyl/dashboard"
        },
	{
            "type": "vcs",
            "url": "https://github.com/Komeyl-dev/Laravel-dashboard"
        },
    ],

    "extra": {
        "laravel": {
            "providers": [
                "Komeyl\\Dashboard\\DashboardServiceProvider"
            ]
        }
    }
}
